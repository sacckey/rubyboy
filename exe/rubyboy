#!/usr/bin/env ruby
# frozen_string_literal: true

require 'rubyboy'
require 'bench'

arg = ARGV[0]

puts "Ruby: #{RUBY_VERSION}"
puts "YJIT: #{RubyVM::YJIT.enabled?}"

if arg == 'bench'
  Rubyboy::Bench.new.bench
elsif arg == 'stackprof'
  Rubyboy::Bench.new.stackprof
else
  Rubyboy::Emulator.new(arg).start
end
